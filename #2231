#include<iostream>
using namespace std;
int divide(int a);
int main() { int num; int a;
 scanf("%d", &num);
 for (int i = 1; i < num; i++) {  if (divide(i) == num) { cout << i; return 0; } }  cout << 0;}
int divide(int a) { int temp = a; while (a != 0) {  temp += (a % 10);  a = a / 10; } return temp;}
